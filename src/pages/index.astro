---
import Layout from "../layouts/Layout.astro";
import Hero from "../components/Hero.astro";
import { Navbar } from "../components/Navbar.jsx";
import { ChooseUs } from "../components/ChooseUs.jsx";
import { Footer } from "../components/Footer.jsx";
import Modules from "../components/Modules.astro";
import TryUs from "../components/TryUs.astro";
import "../styles/Theme.css";
const { pathname } = Astro.url;
---

<Layout title="Exseed Technologies">
  <Navbar pathname={pathname} client:load />
  <Hero />
  <ChooseUs />
  <Modules />
  <TryUs />
  <Footer aboutUs={false} />
</Layout>
